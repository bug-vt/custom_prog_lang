func main
{
  var x
  mov x, 7
  print x
  print "\n"

  var x_ref
  ref x_ref, x
  print x_ref
  print "\n"

  push x_ref
  call firstFunc
}

func firstFunc
{
  param x_ref
  var y
  mem y, x_ref, 0
  print y
  print "\n"

  push x_ref
  call secondFunc
}

func secondFunc
{
  param x_ref
  var y
  mem y, x_ref, 0
  print y
  print "\n"
}
